<thlogic>
    <attr sel="#headId"
          th:replace="~{fragments/head :: headFragment}"/>
    <attr sel="#main-message"
          th:text="${__${T(example.spring.multimodule.util.AttributeNames).MAIN_MESSAGE}__}"/>
    <attr sel="#result-message"
          th:text="${__${T(example.spring.multimodule.util.AttributeNames).RESULT_MESSAGE}__}"/>
    <attr sel="#game-form"
          th:action="@{__${T(example.spring.multimodule.util.GameMapping).PLAY}__}"/>
    <attr sel="#footerId"
          th:replace="~{fragments/footer :: footer}"/>
</thlogic>